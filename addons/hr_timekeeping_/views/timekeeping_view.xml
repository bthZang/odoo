<odoo>

<record id="timekeeping_view_tree" model="ir.ui.view">
		<field name="name">timekeeping.tree</field>
		<field name="model">timekeeping</field>
		<field name="arch" type="xml">
			<tree string="Timekeeping">
				<field name="name" />
				<field name="address" />
				<field name="work_phone" />
				<field name="work_email" />
			</tree>
		</field>
	</record>

	<record id="timekeeping_view_form" model="ir.ui.view">
		<field name="name">timekeeping.form</field>
		<field name="model">timekeeping</field>
		<field name="arch" type="xml">
			<form string="Timekeeping">
				<sheet>
					<group>
						<group>
							<field name="name" />
							<field name="address" />
						</group>
						<group>
							<field name="work_phone" />
							<field name="work_email" />
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="timekeeping_action" model="ir.actions.act_window">
		<field name="name">Timekeeping</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">timekeeping</field>
	</record>

	<!-- <menuitem id="menu" name="Recruitment" />
	<menuitem id="menu_recruitment" name="Recruitment" parent="menu" action='recruitment_action' /> -->
</odoo>